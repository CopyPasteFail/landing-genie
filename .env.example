# Root domain that you own and will point to Cloudflare
ROOT_DOMAIN=your_tld_here.com

# Cloudflare settings
CLOUDFLARE_ACCOUNT_ID=your_account_id_here
CLOUDFLARE_API_TOKEN=your_api_token_here

# Gemini and models
GEMINI_CODE_MODEL=gemini-2.5-flash # or gemini-2.5-pro
GEMINI_IMAGE_MODEL=gemini-2.5-flash-image
GEMINI_IMAGE_COST_PER_1K_TOKENS=0.03
GEMINI_CLI_COMMAND=gemini

GEMINI_API_KEY=

# GEMINI_ALLOW_CLI_API_KEY=1


GEMINI_TELEMETRY_OTLP_ENDPOINT=

# Logging and follow-up tuning
# LANDING_GENIE_PROMPT_LOG_PATH=.log/
# LANDING_GENIE_PROMPT_LOG_MAX_MB=5
# LANDING_GENIE_MAX_FOLLOW_UP_QUESTIONS=20
# LANDING_GENIE_MAX_IMAGE_FOLLOW_UP_QUESTIONS=20
