# Root domain that you own and will point to Cloudflare
ROOT_DOMAIN=ai-lab.life

# Cloudflare settings
CLOUDFLARE_ACCOUNT_ID=your_account_id_here
CLOUDFLARE_API_TOKEN=your_api_token_here

# Gemini and models
GEMINI_CODE_MODEL=gemini-2.5-pro
GEMINI_IMAGE_MODEL=gemini-3-pro-image-preview # or gemini-2.5-flash-image
GEMINI_CLI_COMMAND=gemini

# Billing-enabled API key used by Python image generation (not passed to the Gemini CLI unless you opt in)
GEMINI_API_KEY=
# Uncomment to pass GEMINI_API_KEY through to the Gemini CLI as well (otherwise text generation uses CLI auth/login)
# GEMINI_ALLOW_CLI_API_KEY=1

# Optional: set to your OTLP collector to enable full telemetry export when landing-genie runs the Gemini CLI
# Leave blank to rely on the CLI settings file for interactive use
GEMINI_TELEMETRY_OTLP_ENDPOINT=
