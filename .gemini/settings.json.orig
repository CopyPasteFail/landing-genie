{
  "$schema": "https://raw.githubusercontent.com/google-gemini/gemini-cli/main/schemas/settings.schema.json",
  "model": "gemini-2.5-pro",
  "contextFileName": "GEMINI.md",
  "tools": {
    "core": [
      "ReadFileTool",
      "GlobTool",
      "WriteFileTool",
      "ReplaceTextTool",
      "google_web_search"
    ],
    "exclude": [
      "run_shell_command",
      "McpServer",
      "TodosTool",
      "MemoryTool"
    ]
  },
  "telemetry": {
    "enabled": true,
    "logPrompts": false,
    "target": "local"
  },
  "security": {
    "disableYoloMode": false
  }
}
